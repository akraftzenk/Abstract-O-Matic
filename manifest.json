{
  "name": "Extension Demo",
  "version": "1.0",
  "description": "Build an Basic Extension!",
  "permissions": ["activeTab", "contextMenus", "menus"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": ["popup.html"],
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  }
}